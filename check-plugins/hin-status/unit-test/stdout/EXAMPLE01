    <script type="application/ld+json" class="yoast-schema-graph">{"@context":"https://schema.org","@graph":[{"@type":["WebPage","FAQPage"],"@id":"https://support.hin.ch/de/","url":"https://support.hin.ch/de/","name":"Support - HIN Support","isPartOf":{"@id":"https://support.hin.ch/de/#website"},"primaryImageOfPage":{"@id":"https://support.hin.ch/de/#primaryimage"},"image":{"@id":"https://support.hin.ch/de/#primaryimage"},"thumbnailUrl":"https://support.hin.ch/content/uploads/2020/01/icon-tutorials.svg","datePublished":"2019-10-29T20:38:55+00:00","dateModified":"2025-01-08T14:56:25+00:00","breadcrumb":{"@id":"https://support.hin.ch/de/#breadcrumb"},"inLanguage":"de-CH","potentialAction":[{"@type":"ReadAction","target":["https://support.hin.ch/de/"]}]},{"@type":"ImageObject","inLanguage":"de-CH","@id":"https://support.hin.ch/de/#primaryimage","url":"https://support.hin.ch/content/uploads/2020/01/icon-tutorials.svg","contentUrl":"https://support.hin.ch/content/uploads/2020/01/icon-tutorials.svg"},{"@type":"BreadcrumbList","@id":"https://support.hin.ch/de/#breadcrumb","itemListElement":[{"@type":"ListItem","position":1,"name":"Support"}]},{"@type":"WebSite","@id":"https://support.hin.ch/de/#website","url":"https://support.hin.ch/de/","name":"HIN Support","description":"","potentialAction":[{"@type":"SearchAction","target":{"@type":"EntryPoint","urlTemplate":"https://support.hin.ch/de/?s={search_term_string}"},"query-input":{"@type":"PropertyValueSpecification","valueRequired":true,"valueName":"search_term_string"}}],"inLanguage":"de-CH"}]}</script>
